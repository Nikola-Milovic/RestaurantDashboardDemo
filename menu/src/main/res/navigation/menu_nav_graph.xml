<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/menu_nav_graph"
    app:startDestination="@id/menuFragment">

    <fragment
        android:id="@+id/menuFragment"
        android:name="com.nikolam.menu.ui.menu.MenuFragment"
        android:label="menu_fragment"
        tools:layout="@layout/menu_fragment" >
        <action
            android:id="@+id/action_menuFragment_to_menuItemDetailFragment2"
            app:destination="@id/menuItemDetailFragment" />
    </fragment>
    <fragment
        android:id="@+id/menuItemDetailFragment"
        android:name="com.nikolam.menu.ui.detail.ItemDetailFragment"
        android:label="MenuItemDetailFragment" >
        <argument
            android:name="MenuItemID"
            app:argType="string" />
    </fragment>
</navigation>